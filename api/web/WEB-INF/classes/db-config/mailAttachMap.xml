<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="MailAttach">

  <!-- Use type aliases to avoid typing the full classname every time. -->
	<typeAlias alias="attach" type="com.terracetech.tims.webmail.mail.vo.MailBigAttachVO" />
	
	<resultMap id="mailAttachResult" class="attach" >
    	<result column="FILE_NAME" property="fileName" jdbcType="VARCHAR" />
	    <result column="FOLDER_PATH" property="folderPath" jdbcType="VARCHAR" />
	    <result column="MAIL_USER_SEQ" property="mailUserSeq" jdbcType="INTEGER" />
	    <result column="MESSAGE_UID" property="messageUid" jdbcType="VARCHAR" />
	    <result column="FILE_SIZE" property="fileSize" jdbcType="VARCHAR" />
	    <result column="REGIST_TIME" property="registTime" jdbcType="VARCHAR" />
	    <result column="EXPIRE_TIME" property="expireTime" jdbcType="VARCHAR" />
	    <result column="DOWNLOAD_COUNT" property="downloadCount" jdbcType="INTEGER" />
	    <result column="ATTACH_FLAG" property="attachFlag" jdbcType="VARCHAR" />
	</resultMap>
	
</sqlMap>