<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="Letter">

	<typeAlias alias="letter" type="com.terracetech.tims.webmail.mail.vo.LetterVO" />
	
  	<resultMap id="letterResult" class="letter">
		<result property="letterSeq" column="letter_seq" />		
		<result property="attachMode" column="attach_mode" />
		<result property="thumbnailImgName" column="thumbnail_img_name" />
		<result property="headImgName" column="head_img_name" />
		<result property="bodyImgName" column="body_img_name" />
		<result property="tailImgName" column="tail_img_name" />
		<result property="thumbnailImg" column="thumbnail_img" />
		<result property="headImg" column="head_img" />
		<result property="bodyImg" column="body_img" />
		<result property="tailImg" column="tail_img" />
	</resultMap>	

</sqlMap>