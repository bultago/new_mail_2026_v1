<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="MailDomain">
	<typeAlias alias="mailDomain" type="com.terracetech.tims.webmail.mailuser.vo.MailDomainVO" />
	<typeAlias alias="mailDomainGroup" type="com.terracetech.tims.webmail.mailuser.vo.MailDomainGroupVO" />
	<typeAlias alias="mailDomainCode" type="com.terracetech.tims.webmail.mailuser.vo.MailDomainCodeVO" />
	<typeAlias alias="logo" type="com.terracetech.tims.webmail.common.vo.LogoVO" />
	<typeAlias alias="commonLogo" type="com.terracetech.tims.webmail.common.vo.CommonLogoVO" />
	<typeAlias alias="docTemplate" type="com.terracetech.tims.webmail.common.vo.DocTemplateVO" />
	
	<resultMap id="mailDomainResult" class="mailDomain">
		<result property="mailDomainSeq" column="mail_domain_seq"/>
		<result property="mailDomain" column="mail_domain"/>
		<result property="mailDomainName" column="mail_domain_name"/>
		<result property="serviceStartTime" column="service_start_time"/>
		<result property="serviceEndTime" column="service_end_time"/>
		<result property="domainStatus" column="domain_status"/>
		<result property="maxUserCount" column="max_user_count"/>
		<result property="currentUserCount" column="current_user_count"/>
		<result property="passwordMethod" column="password_method"/>
		<result property="mailServices" column="mail_services"/>
		<result property="defaultPspam" column="default_pspam"/>
		<result property="filterApply" column="filter_apply"/>
		<result property="urlAddress" column="url_address"/>
	</resultMap>
	
	<resultMap id="mailDomainHashResult" class="java.util.HashMap">
		<result property="mail_domain_seq" column="mail_domain_seq"/>
		<result property="mail_domain" column="mail_domain"/>
		<result property="mail_domain_name" column="mail_domain_name"/>
		<result property="service_start_time" column="service_start_time"/>
		<result property="service_end_time" column="service_end_time"/>
		<result property="domain_status" column="domain_status"/>
		<result property="max_user_count" column="max_user_count"/>
		<result property="current_user_count" column="current_user_count"/>
		<result property="password_method" column="password_method"/>
		<result property="mail_services" column="mail_services"/>
		<result property="filter_apply" column="filter_apply"/>
		<result property="default_pspam" column="default_pspam"/>
	</resultMap>
	
	<resultMap id="mailDomainGroupHashResult" class="java.util.HashMap">
		<result property="mail_domain_seq" column="mail_domain_seq"/>
		<result property="mail_group_seq" column="mail_group_seq"/>
		<result property="mail_group" column="mail_group"/>
		<result property="mail_group_name" column="mail_group_name"/>
		<result property="mail_quota" column="mail_quota"/>
		<result property="quota_overlook_ratio" column="quota_overlook_ratio"/>
		<result property="quota_warning_mode" column="quota_warning_mode"/>
		<result property="quota_warning_ratio" column="quota_warning_ratio"/>
		<result property="quota_violation_action" column="quota_violation_action"/>
		<result property="big_attach_quota" column="big_attach_quota"/>
		<result property="web_folder_quota" column="web_folder_quota"/>
