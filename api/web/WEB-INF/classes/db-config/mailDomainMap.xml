	</resultMap>
	
	<resultMap id="localDomainResult"  class="java.util.HashMap">
   		<result property="key"  column="dname"/>
   		<result property="value"  column="mail_domain_seq" javaType="Long"/>
   	</resultMap>
   	
   	<resultMap id="logoResult" class="logo">
		<result property="mailDomainSeq" column="mail_domain_seq"/>
		<result property="logoImgName" column="logo_img_name"/>
		<result property="logoImg" column="logo_img"/>
		<result property="title" column="title"/>
		<result property="copyright" column="copyright"/>
	</resultMap>
	
	<resultMap id="commonLogoResult" class="commonLogo">
		<result property="imgMode" column="img_mode"/>
		<result property="logoImgName" column="logo_img_name"/>
		<result property="logoImg" column="logo_img"/>		
	</resultMap>
	
	<resultMap id="docTemplateResult" class="docTemplate">
		<result property="docTemplateSeq" column="doc_template_seq" />		
		<result property="name" column="doc_template_name" />
		<result property="description" column="doc_template_description" />
		<result property="template" column="doc_template" />
	</resultMap>
	
	<resultMap id="mailDomainDormantMonth" class="java.util.HashMap">
		<result property="long_term_manage" column="long_term_manage"/>
		<result property="long_term_month" column="long_term_month"/>
		<result property="long_term_change_month" column="long_term_change_month"/>
	</resultMap>
</sqlMap>