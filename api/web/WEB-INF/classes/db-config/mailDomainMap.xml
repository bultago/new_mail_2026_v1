		<result property="sent_expire_days" column="sent_expire_days"/>
		<result property="trash_expire_days" column="trash_expire_days"/>
		<result property="spam_expire_days" column="spam_expire_days"/>
		<result property="user_expire_days" column="user_expire_days"/>
		<result property="delivery_mode" column="delivery_mode"/>
		<result property="description" column="description"/>
		<result property="note_use" column="note_use"/>
	</resultMap>
	
	<resultMap id="mailDomainGroupResult" class="mailDomainGroup">
		<result property="mailDomainSeq" column="mail_domain_seq"/>
		<result property="mailGroupSeq" column="mail_group_seq"/>
		<result property="mailGroup" column="mail_group"/>
		<result property="mailGroupName" column="mail_group_name"/>
		<result property="mailQuota" column="mail_quota"/>
