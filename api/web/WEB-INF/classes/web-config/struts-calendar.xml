<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="dynamicScheduler" namespace="/dynamic/scheduler" extends="tims-default">
    	<action name="schedulerCommon"
					class="schedulerCommonAction"
					method="loadPage">
			<result name="load">/dynamic/scheduler/schedulerFrame.jsp</result>
		</action>
		<action name="monthScheduler"
					class="monthSchedulerAction">
			<result name="success">/dynamic/scheduler/monthScheduler.jsp</result>
		</action>
		<action name="weekScheduler"
					class="weekSchedulerAction">
			<result name="success">/dynamic/scheduler/weekScheduler.jsp</result>
		</action>
		<action name="dayScheduler"
					class="daySchedulerAction">
			<result name="success">/dynamic/scheduler/dayScheduler.jsp</result>
		</action>
		<action name="progressWeekScheduler"
					class="progressSchedulerAction">
			<result name="success">/dynamic/scheduler/progressWeekScheduler.jsp</result>
		</action>
		<action name="progressDayScheduler"
					class="progressSchedulerAction"
					method="executeDay">
			<result name="success">/dynamic/scheduler/progressDayScheduler.jsp</result>
		</action>
		<action name="schedulerCalendar"
					class="monthSchedulerAction">
			<result name="success">/dynamic/scheduler/schedulerCalendar.jsp</result>
		</action>
		
		<action name="schedulerOutlookLogin" class="schedulerOutlookLoginAction"/>
		
		<action name="schedulerOutlookSync" class="schedulerOutlookSyncAction"/>
		
		<action name="schedulerIdOutlookSync" class="schedulerOutlookSyncAction" method="executePart"/>
		
		<action name="schedulerShareIdOutlookSync" class="schedulerOutlookSyncAction" method="executeShare"/>
		
		<action name="schedulerOutlookReceive" class="schedulerOutlookReceiveAction"/>
		
		<action name="schedulerOutlookUpdate" class="schedulerOutlookUpdateAction"/>
		
		<action name="schedulerOutlookSso"
					class="schedulerOutlookSsoAction">
			<result name="success">/common/redirect.jsp</result>
			<result name="fail">/common/alertProcess.jsp</result>
		</action>
    </package>
</struts>