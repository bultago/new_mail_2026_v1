<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="portlet" namespace="/portlet" extends="tims-default">
    	<action 	name="noticeView"
    				class="listContentAction">
    		<result name="success">/dynamic/portlet/noticeList.jsp</result>
    	</action>
    	<action 	name="calendarView"
    				class="monthSchedulerAction">
    		<result name="success">/dynamic/portlet/calendar.jsp</result>
    	</action>
    	<action 	name="quotaView"
    				class="quotaViewAction">
    		<result name="success">/dynamic/portlet/quotaView.jsp</result>
    	</action>
    	
    	<action 	name="mailView"
    				class="listMessageAction">
    		<result name="mailPortletList">/dynamic/portlet/mailList.jsp</result>    		
    	</action>
    	
    	<action 	name="todaySchedule"
    				class="schedulerCommonAction">
    		<result name="success">/dynamic/portlet/todaySchedule.jsp</result>
    	</action>
    	
    	<action 	name="clockView"
    				class="schedulerCommonAction">
    		<result name="success">/dynamic/portlet/clock.jsp</result>
    	</action>
    </package>
</struts>