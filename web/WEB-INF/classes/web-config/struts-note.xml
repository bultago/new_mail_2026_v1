<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="note" namespace="/dynamic/note" extends="tims-default">
   		<action name="noteCommon" class="noteCommonAction">
   			<result name="success">/dynamic/note/noteFrame.jsp</result>
   		</action>
   		
   		<action name="noteInfo" class="noteInfoAction"/>
   		
   		<action name="noteWrite" class="noteWriteAction">
   			<result name="success">/dynamic/note/noteWrite.jsp</result>
   		</action>
   		
   		<action name="noteList" class="noteListAction"/>
   		
   		<action name="noteRead" class="noteReadAction">
   			<result name="success">/dynamic/note/noteRead.jsp</result>
   		</action>
   		
   		<action name="moveSave" class="noteWorkAction"/>
   		
   		<action name="noteDelete" class="noteWorkAction" method="deleteNote"/>
   		
   		<action name="noteSetting" class="noteSettingAction">
   			<result name="success">/dynamic/note/noteSetting.jsp</result>
   		</action>
   		
   		<action name="searchUserList" class="searchUserListAction"/>
   		
   		<action name="noteSend" class="noteSendAction"/>
   		
   		<action name="noteSave" class="noteWorkAction"/>
   		
   		<action name="noteDelete" class="noteWorkAction" method="deleteNote"/>
   		
   		<action name="noteReject" class="noteWorkAction" method="rejectNote"/>
   		
   		<action name="noteAllSelect" class="noteAllSelectAction"/>
   		
   		<action name="noteMdnInfo" class="noteMdnAction"/>
   		
   		<action name="noteRecallMdn" class="noteMdnAction" method="recallMdn"/>
   		
   		<action name="noteSaveSetting" class="noteSettingAction" method="executeSave"/>
    </package>
</struts>