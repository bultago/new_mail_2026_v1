	<typeAlias alias="SchedulerAssetPlan" type="com.terracetech.tims.webmail.scheduler.vo.SchedulerAssetPlanVO" />
	<typeAlias alias="SchedulerAssetCategoryImage" type="com.terracetech.tims.webmail.scheduler.vo.SchedulerAssetCategoryImageVO" />
	
	
	<resultMap id="schedulerResult" class="scheduler">
		<result property="schedulerId" column="scheduler_id" />
		<result property="mailUserSeq" column="mail_user_seq" />
		<result property="startDate" column="start_date" />
