<sqlMap namespace="Scheduler">

	<typeAlias alias="scheduler" type="com.terracetech.tims.webmail.scheduler.vo.SchedulerDataVO" />
	<typeAlias alias="schedulerHoliday" type="com.terracetech.tims.webmail.scheduler.vo.SchedulerHolidayVO" />
	<typeAlias alias="schedulerShare" type="com.terracetech.tims.webmail.scheduler.vo.SchedulerShareVO" />
