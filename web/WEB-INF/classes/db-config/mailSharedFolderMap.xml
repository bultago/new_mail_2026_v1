<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="MailSharedFolder">

	<typeAlias alias="sharedFolder" type="com.terracetech.tims.webmail.mail.vo.SharedFolderVO" />
	<typeAlias alias="sharedUser" type="com.terracetech.tims.webmail.mail.vo.SharedFolderUserVO" />
	
  	<resultMap id="sharedFolderResult" class="sharedFolder">
		<result property="folderUid" column="folder_uid" />		
		<result property="sharedUserSeq" column="mail_user_seq" />
		<result property="folderName" column="folder_name" />
		<result property="sharedUserName" column="user_name" />
		<result property="sharedUserId" column="mail_uid" />		
	</resultMap>
	
	<resultMap id="sharedFolderList" class="sharedFolder">
		<result property="folderUid" column="folder_uid" />		
		<result property="sharedUserSeq" column="mail_user_seq" />
		<result property="folderName" column="folder_name" />				
	</resultMap>
	
	<resultMap id="sharedFolderUserResult" class="sharedUser">
		<result property="userSeq" column="mail_user_seq" />
		<result property="userId" column="mail_uid" />		
		<result property="userName" column="user_name" />		
	</resultMap>

</sqlMap>